{
  "name": "nf-workflow-ui",
  "version": "1.0.0",
  "description": "Workflow User Interface",
  "proxy": {
    "/api": {
      "target": "http://localhost:3001"
    }
  },
  "scripts": {
    "test": "mocha --reporter nyan --compilers js:babel-core/register --recursive --check-leaks",
    "start": "react-scripts start",
    "build-old": "gulp build --release",
    "build": "react-scripts build && (rm -rf ./server/public/build | true) && cp -r build server/public",
    "deploy": "gulp deploy --release",
    "eslint": "eslint  [src]*",
    "server": "nodemon ./server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Netflix/conductor.git"
  },
  "keywords": [
    "Workflow",
    "conductor"
  ],
  "license": "SEE LICENSE IN license.txt",
  "dependencies": {
    "autoprefixer": "^6.0.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.3.14",
    "bootstrap": "3.3.6",
    "bunyan": "^1.7.1",
    "clipboard": "^1.6.1",
    "create-react-class": "^15.5.3",
    "d3": "^3.5.16",
    "dagre-d3": "^0.4.17",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "fbjs": "^0.7.2",
    "font-awesome": "^4.7.0",
    "graceful-fs": "^4.0.0",
    "gulp-env": "^0.4.0",
    "history": "^1.17.0",
    "lodash": "^4.0.0",
    "lodash.merge": "^4.3.0",
    "moment": "^2.17.1",
    "prop-types": "^15.5.9",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.2",
    "react-bootstrap-table": "^1.5.3",
    "react-bootstrap-typeahead": "^0.9.2",
    "react-burger-menu": "^1.5.2",
    "react-d3-basic": "^1.5.2",
    "react-dom": "^0.14.0",
    "react-redux": "^4.4.0",
    "react-router": "^1.0.3",
    "react-router-redux": "^4.0.0",
    "react-select": "^1.0.0-beta10",
    "react-toastr": "2.9.3",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "underscore": "^1.8.3",
    "uuid": "^3.3.2",
    "webpack": "^1.12.10"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^2.5.3",
    "nodemon": "^1.18.3"
  }
}
